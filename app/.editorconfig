# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
charset = utf-8
indent_style = space

# 4 space indentation
[*.js]
indent_size = 2

[*.{pcss,twig}]
indent_size = 4
